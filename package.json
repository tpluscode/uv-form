{
  "name": "@tpluscode/uv-form",
  "version": "0.0.0",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "lint": "eslint . --ext .ts,.js --quiet",
    "test": "jest --coverage",
    "prepack": "npm run build",
    "start": "node -r esm example/index.js",
    "watch": "tsc --watch",
    "web": "webpack-dev-server --mode development",
    "build": "webpack --mode production"
  },
  "files": [
    "dist",
    "*.d.ts",
    "*.d.ts.map"
  ],
  "dependencies": {
    "@tpluscode/rdf-ns-builders": "^0.1.0",
    "@tpluscode/rdfine": "^0.4.12",
    "@tpluscode/rdf-string": "^0.2.11",
    "@rdfine/shacl": "^0.1.1",
    "@rdfjs/namespace": "^1.1.0",
    "@rdfjs/parser-n3": "^1.1.0",
    "clownface": "^0.12.3",
    "lit-element": "^2.3.1",
    "lit-html": "^1.2.1",
    "rdf-ext": "^1",
    "rdf-utils-fs": "^2",
    "string-to-stream": "^3.0.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@open-wc/building-webpack": "^2.13.16",
    "@tpluscode/eslint-config": "0.1.0",
    "@types/clownface": "^0.12.13",
    "@types/jest": "^25.1.2",
    "@types/rdf-ext": "^1",
    "@types/rdf-js": "^2.0.12",
    "@types/rdfjs__formats-common": "^2.0.0",
    "@types/rdfjs__parser-n3": "^1.1.2",
    "@types/rdfjs__namespace": "^1.1.1",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "cz-conventional-changelog": "^3.0.2",
    "eslint-plugin-jest": "^23.1.1",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.7",
    "standard": "^14.3.1",
    "ts-helpers": "^1.1.2",
    "ts-node": "^8.9.1",
    "typescript": "^3.8.3",
    "webpack-merge": "^4.2.2",
    "webpack-dev-server": "^3.10.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tpluscode/ts-template.git"
  },
  "author": "Tomasz Pluskiewicz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tpluscode/ts-template/issues"
  },
  "homepage": "https://github.com/tpluscode/ts-template#readme",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged && tsc --noEmit"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix --quiet"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>"
    ],
    "testRegex": "tests/.+\\.ts$"
  }
}
